{
  "name": "Video Blend Text Effect",
  "short_name": "VidBlend",
  "description": "Create customizable text effects with vibration and animation over video backgrounds",
  "start_url": "/insert/",
  "display": "standalone",
  "background_color": "#000000",
  "theme_color": "#000000",
  "orientation": "any",
  "icons": [
    {
      "src": "/insert/icon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "categories": ["graphics", "video", "utilities"],
  "screenshots": [
    {
      "src": "/insert/screenshot-mobile.png",
      "sizes": "540x720",
      "type": "image/png",
      "form_factor": "narrow"
    },
    {
      "src": "/insert/screenshot-desktop.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide"
    }
  ],
  "share_target": {
    "action": "/insert/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "video",
          "accept": ["video/*"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/insert/",
      "accept": {
        "video/*": [".mp4", ".webm", ".mov"]
      }
    }
  ]
}
